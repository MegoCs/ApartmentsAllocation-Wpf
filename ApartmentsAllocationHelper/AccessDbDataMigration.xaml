<Window x:Class="ApartmentsAllocationHelper.AccessDbDataMigration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApartmentsAllocationHelper"
        mc:Ignorable="d"
        Title="ادراج بيانات عملاء من برنامج الادارة" Height="250" Width="350" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" FontSize="16" FontWeight="Bold">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <StackPanel Margin="10" Width="300" Height="auto" Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Name="selectDbFileBtn" Margin="10" Content="اختيار ملف البيانات" HorizontalAlignment="Right" VerticalAlignment="Top" FontSize="16" Click="SelectDbFileBtn_Click" />
            <ProgressBar Name="progress" Margin="10" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="30" />
            <TextBlock Name="DescriptionDetailsTxt" Visibility="Collapsed" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="200" TextAlignment="Center" Text="يتم نقل بيانات الاعضاء الذين لا يوجد لهم بيانات وارقام قومية ف البيانات " TextWrapping="Wrap" FontSize="16"/>
        </StackPanel>
    </Grid>
</Window>
